                  <section class="py-5">
    <style>
        /* Banner Ads Section Styles */
        .py-5 {
            position: relative;
            overflow: hidden;
        }

        .container-fluid {
            padding: 0 15px;
        }

        .row {
            gap: 30px;
            margin: 0;
        }

        @media (min-width: 768px) {
            .row {
                gap: 0;
            }
        }

        /* Banner Ad Common Styles */
        .banner-ad {
            position: relative;
            border-radius: 25px;
            overflow: hidden;
            min-height: 400px;
            background-size: contain !important;
            background-position: right center !important;
            transition: all 0.5s cubic-bezier(0.175, 0.885, 0.32, 1.275);
            box-shadow: 0 15px 35px rgba(0, 0, 0, 0.1);
            border: 1px solid rgba(255, 255, 255, 0.2);
        }

        .banner-ad::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(135deg, rgba(0,0,0,0.4) 0%, transparent 50%);
            opacity: 0.8;
            transition: all 0.5s ease;
            z-index: 1;
        }

        .banner-ad::after {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background: linear-gradient(45deg, transparent 0%, rgba(255,255,255,0.1) 50%, transparent 100%);
            opacity: 0;
            transition: opacity 0.5s ease;
            z-index: 2;
        }

        .banner-ad:hover {
            transform: translateY(-15px) scale(1.02);
            box-shadow: 0 25px 60px rgba(0, 0, 0, 0.25);
        }

        .banner-ad:hover::before {
            opacity: 0.6;
            background: linear-gradient(135deg, rgba(0,0,0,0.6) 0%, transparent 60%);
        }

        .banner-ad:hover::after {
            opacity: 1;
        }

        /* First Banner Specific Styles */
        .banner-ad.bg-danger {
            background: linear-gradient(135deg, #e74c3c 0%, #c0392b 100%) !important;
        }

        .banner-ad.bg-danger::before {
            background: linear-gradient(135deg, rgba(231, 76, 60, 0.8) 0%, transparent 60%) !important;
        }

        /* Second Banner Specific Styles */
        .banner-ad.bg-info {
            background: linear-gradient(135deg, #3498db 0%, #2980b9 100%) !important;
        }

        .banner-ad.bg-info::before {
            background: linear-gradient(135deg, rgba(52, 152, 219, 0.8) 0%, transparent 60%) !important;
        }

        /* Banner Content */
        .banner-content {
            position: relative;
            z-index: 3;
            height: 100%;
            display: flex;
            flex-direction: column;
            justify-content: center;
            max-width: 60%;
            animation: contentSlideIn 1s ease-out;
        }

        @keyframes contentSlideIn {
            from {
                opacity: 0;
                transform: translateX(-30px);
            }
            to {
                opacity: 1;
                transform: translateX(0);
            }
        }

        /* Categories Badge */
        .categories {
            font-family: 'Poppins', sans-serif;
            background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
            margin-bottom: 15px;
            text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
            position: relative;
            display: inline-block;
            padding: 5px 0;
        }

        .categories::after {
            content: '';
            position: absolute;
            bottom: 0;
            left: 0;
            width: 0;
            height: 3px;
            background: linear-gradient(135deg, #ffffff 0%, #f8f9fa 100%);
            border-radius: 2px;
            transition: width 0.5s ease;
        }

        .banner-ad:hover .categories::after {
            width: 100%;
        }

        /* Banner Title */
        .banner-title {
            font-family: 'Cinzel', serif;
            font-size: 2.5rem;
            font-weight: 700;
            color: #ffffff;
            margin-bottom: 15px;
            text-shadow: 2px 2px 8px rgba(0, 0, 0, 0.5);
            line-height: 1.2;
            transition: all 0.3s ease;
        }

        .banner-ad:hover .banner-title {
            transform: translateX(10px);
            text-shadow: 3px 3px 12px rgba(0, 0, 0, 0.7);
        }

        /* Banner Description */
        .banner-content p {
            font-family: 'Poppins', sans-serif;
            font-size: 1.1rem;
            color: rgba(255, 255, 255, 0.9);
            margin-bottom: 25px;
            line-height: 1.6;
            text-shadow: 1px 1px 3px rgba(0, 0, 0, 0.3);
            transition: all 0.3s ease;
        }

        .banner-ad:hover .banner-content p {
            transform: translateX(10px);
            color: rgba(255, 255, 255, 1);
        }

        /* Shop Now Button */
        .btn-dark {
            background: linear-gradient(135deg, #2c3e50 0%, #34495e 100%);
            border: none;
            border-radius: 50px;
            padding: 15px 35px;
            font-family: 'Poppins', sans-serif;
            font-weight: 600;
            font-size: 1rem;
            text-transform: uppercase;
            letter-spacing: 1.5px;
            transition: all 0.4s ease;
            box-shadow: 0 8px 25px rgba(0, 0, 0, 0.3);
            position: relative;
            overflow: hidden;
            width: fit-content;
            z-index: 4;
        }

        .btn-dark::before {
            content: '';
            position: absolute;
            top: 0;
            left: -100%;
            width: 100%;
            height: 100%;
            background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
            transition: left 0.6s ease;
        }

        .btn-dark:hover {
            transform: translateY(-3px) scale(1.05);
            box-shadow: 0 15px 35px rgba(0, 0, 0, 0.4);
            background: linear-gradient(135deg, #34495e 0%, #2c3e50 100%);
        }

        .btn-dark:hover::before {
            left: 100%;
        }

        /* Floating Elements */
        .floating-watch {
            position: absolute;
            z-index: 2;
            opacity: 0.1;
            animation: floatWatch 6s ease-in-out infinite;
        }

        .banner-ad:nth-child(1) .floating-watch {
            top: 20%;
            right: 15%;
            width: 80px;
            height: 80px;
            animation-delay: 0s;
        }

        .banner-ad:nth-child(2) .floating-watch {
            bottom: 25%;
            right: 20%;
            width: 60px;
            height: 60px;
            animation-delay: 2s;
        }

        @keyframes floatWatch {
            0%, 100% {
                transform: translateY(0) rotate(0deg);
            }
            33% {
                transform: translateY(-20px) rotate(10deg);
            }
            66% {
                transform: translateY(10px) rotate(-5deg);
            }
        }

        /* Discount Pulse Effect */
        .categories {
            position: relative;
        }

        .categories::before {
            content: '🔥';
            position: absolute;
            left: -35px;
            top: 50%;
            transform: translateY(-50%);
            font-size: 1.5rem;
            animation: firePulse 2s ease-in-out infinite;
        }

        @keyframes firePulse {
            0%, 100% {
                transform: translateY(-50%) scale(1);
            }
            50% {
                transform: translateY(-50%) scale(1.2);
            }
        }

        /* Responsive Design */
        @media (max-width: 768px) {
            .row {
                flex-direction: column;
                gap: 25px;
            }

            .banner-ad {
                min-height: 350px;
                background-size: cover !important;
                background-position: center right !important;
            }

            .banner-content {
                max-width: 80%;
                padding: 3rem !important;
            }

            .banner-title {
                font-size: 2rem;
            }

            .categories {
                font-size: 1.8rem !important;
            }

            .banner-content p {
                font-size: 1rem;
            }

            .btn-dark {
                padding: 12px 30px;
                font-size: 0.9rem;
            }
        }

        @media (max-width: 576px) {
            .banner-ad {
                min-height: 300px;
            }

            .banner-content {
                max-width: 90%;
                padding: 2rem !important;
            }

            .banner-title {
                font-size: 1.8rem;
            }

            .categories {
                font-size: 1.5rem !important;
            }

            .banner-content p {
                font-size: 0.95rem;
